std :: @import("std")

pub main :: fn() !void {
    // Basic variables
    a := 42
    b := 3.14

    // Print basic values
    std.debug.print("Integer: {}\n", .{a})
    std.debug.print("Float: {f}\n", .{b})
    
    // Test arrays  
    numbers :: [1, 2, 3, 4, 5]
    sum := 0
    for (numbers) |num| {
        sum += num  
    }
    
    std.debug.print("Array sum: {}\n", .{sum})
    
    // Test simple calculations
    result := a * 2 + 10
    std.debug.print("Result: {}\n", .{result})
    
    return
}
// Test 'comp' keyword for compile-time functions

std :: @import("std")

// Regular runtime function
add :: fn (a: i32, b: i32) i32 {
    return a + b
}

// Compile-time function
myFunc :: comp fn (a: i32, b: i32) i32 {
    return a * 2 + b
}

// Another compile-time function
calculate :: comp fn (x: i32) i32 {
    return x * x + 10
}

main :: fn () void {
    result :: add(5, 3)
    std.debug.print("Runtime result: {}\n", .{result})
    
    // Note: compile-time functions should not generate runtime code
    std.debug.print("Compile-time functions test completed\n", .{})
}
// Simple test to demonstrate compile-time execution
Person :: struct {
    name: string,
    age: i32,
}

// Simple compile-time function that returns a constant
getPersonFieldCount :: comp fn () i32 {
    return @fieldCount(Person)  // Should be computed at compile-time as 2
}

// Test
main :: fn () void {
    count := getPersonFieldCount()  // This should be evaluated at compile-time
    std.debug.print("Runtime only: field count was computed at compile-time\n")
}
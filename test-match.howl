std :: @import("std")

MyError :: error { 
    InvalidValue,
}

createValue :: fn (a: i32) MyError!i32 {
    match a {
    |   < 0 => {
                return MyError.InvalidValue
            }
    |    _   => {
                return a * 2
            }
    }
}

main :: fn () !void {
    result :: try createValue(5)
    std.debug.print("Result: {}\n", .{result})
}
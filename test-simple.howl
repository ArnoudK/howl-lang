MyError :: error { 
    DivisionByZero,
    UnknownError,
    InvalidValue,
}

MyStruct :: struct {
    field1: i32,
    field2: f64,
}

createMyStruct :: fn (a: i32, b: f64) MyError!MyStruct {
    return MyStruct{ .field1 = a, .field2 = b }
}

main :: fn () void {
    my_struct :: try createMyStruct(42, 3.14)
    std.debug.print("MyStruct: field1={}, field2={}\n", .{my_struct.field1, my_struct.field2})
}